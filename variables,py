# aqui hay una variable de tipo entero
numero=147
print(numero) # esta line sierve para imprimir la variable numero
print(type(numero))

# redefinicion la variable numer
numero = 147.5
print(numero)
print(type(numero))

complejo = 1 + 2
print(complejo)
print(type(complejo))

palabra = "hola"
print(palabra)
print(type(palabra))

bandera = True
print(bandera)
print(type(bandera))

vacio = None
print(vacio)
print(type(vacio))